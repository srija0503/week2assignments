def transpose(matrix):
    rows = len(matrix)
    cols = len(matrix[0])
    result = [[0 for _ in range(rows)] for _ in range(cols)]

    for i in range(rows):
        for j in range(cols):
            result[j][i] = matrix[i][j]
    
    return result
mat = [
    [1, 2, 3],
    [4, 5, 6]
]

transposed = transpose(mat)

print("Original:")
for row in mat:
    print(row)

print("\nTranspose:")
for row in transposed:
    print(row)
